{
	"name": "ikilobyte/pulsar-client-php",
	"description": "PHP Native Client library for Apache Pulsar",
	"type": "library",
	"license": "MIT",
	"keywords": [
		"Pulsar",
		"Client",
		"Library"
	],
	"autoload": {
		"psr-4": {
			"Pulsar\\": "src/"
		}
	},
	"require": {
		"php": ">=7.0",
		"google/crc32": "^0.1.0",
		"protobuf-php/protobuf": "^0.1.3",
		"ext-curl": "*"
	},
	"require-dev": {
		"symfony/var-dumper": "^3.4"
	}
}
